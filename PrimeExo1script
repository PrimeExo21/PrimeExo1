local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

local Window = Rayfield:CreateWindow({
    Name = "PrimeExo",
    LoadingTitle = "PrimeExo",
    LoadingSubtitle = "PrimeExo",
    ConfigurationSaving = {
       Enabled = true,
       FolderName = nil, -- Create a custom folder for your hub/game
       FileName = "PrimeExo"
    },
    Discord = {
       Enabled = false,
       Invite = "ABCD", -- The Discord invite code, do not include discord.gg/
       RememberJoins = true -- Set this to false to make them join the discord every time they load it up
    },
    KeySystem = true, -- Set this to true to use our key system
    KeySettings = {
       Title = "PrimeExo",
       Subtitle = "Key System",
       Note = "PrimeExo on top",
       FileName = "SiriusKey",
       SaveKey = true,
       GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
       Key = "PrimeExo"
    }
 })

 _G.PrimeExo = true

function PrimeExo()
 while _G.PrimeExo == true do
    local args = {
    [1] = "swingKatana"
}

game:GetService("Players").LocalPlayer.ninjaEvent:FireServer(unpack(args))
wait(0.1)
end
end
 local Tab = Window:CreateTab("PrimeExo", 4483362458) -- Title, Image
 local Toggle = Tab:CreateToggle({
    Name = "PrimeExo",
    CurrentValue = false,
    Flag = "Toggle1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(Value)
        _G.PrimeExo = Value
        PrimeExo()
    end,
 })
